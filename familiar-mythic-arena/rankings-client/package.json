  "dependencies": {
    "http-proxy-middleware": "^0.20.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-icons": "^3.8.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^4.4.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "npm-check-updates": "^3.2.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.11.0",
    "redux-devtools-extension": "^2.13.8"
  },
  "eslintConfig": {
